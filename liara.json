{
    "app": "modern-dictionary",
    "platform": "laravel",
    "php": "8.3",
    "build": {
        "location": "iran"
    },
    "mounts": [
        {
            "name": "docs",
            "to": "/var/www/html/storage/app/docs"
        }
    ],
    "deploy": {
        "before": [
            "php artisan storage:link",
            "chmod -R 775 storage/app/docs"
        ]
    }
}
