{
    "app": "modern-dictionary",
    "port": 3000,
    "build": {
        "location": "iran"
    },
    "laravel": {
        "phpVersion": "8.3",
        "storage": {
            "disks": {
                "docs": {
                    "driver": "local",
                    "root": "/var/www/html/docs",
                    "visibility": "public"
                }
            }
        }
    },
    "disks": [
        {
            "name": "docs",
            "mountTo": "/var/www/html/docs"
        }
    ]
}
